Package: MicSim
Type: Package
Title: Performing Continuous-Time Microsimulation
Version: 2.0.1
Date: 2024-01-23
Authors@R: c(person("Sabine", "Zinn", role = c("aut","cre"),
    email = "szinn@diw.de"),
    person("Claudio","Bosco", role = "ctb",
    email = "Claudio.BOSCO@ec.europa.eu"),
